{
  "name": "vue-template",
  "version": "1.0.0",
  "description": "Scalable starter boilerplate for VueJS applications",
  "main": "./src/bootstrap.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint --ext .js,.vue src",
    "lint:css": "stylelint '**/*.vue'",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:three11/vue-template.git"
  },
